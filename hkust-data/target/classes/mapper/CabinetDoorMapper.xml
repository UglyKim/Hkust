<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hkust.mapper.CabinetDoorMapper">
    <select id="selectListByStat" parameterType="java.lang.String" resultType="com.hkust.entity.CabinetDoor">
        select *
        from hkust_cabinet_door
        where stat = #{stat};
    </select>
</mapper>